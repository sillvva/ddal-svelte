		"@auth/sveltekit": "^0.3.9",
		"@decs/typeschema": "0.12.0",
		"@prisma/client": "^5.4.0",
		"ioredis": "^5.3.2",
		"minisearch": "^6.1.0",
		"valibot": "^0.18.0"
	},
	"pnpm": {
		"overrides": {
			"@auth/core": "^0.10.0"
		}
	}
}
